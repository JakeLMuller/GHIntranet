@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="eng">
<head>
    <meta charset="utf-8">
    <title>HTML5 Space Invaders</title>
    <script type="text/javascript" src="~/Scripts/SpaceInvaders.js"></script>

   <style>
       body {
          background-color: dimgray;
          text-align: center;
        }

        canvas {
          background-color: black;
          border: 1px solid black;
        }

   </style> 
</head>
<body>
    <!-- We use canvas to draw stuff on the screen. -->
    <canvas id="game-canvas" width="672" height="768">
        Your browser does not support HTML5 canvas.
    </canvas>
    <script>
    var spaceInvaders = new SpaceInvaders.Game();
    spaceInvaders.start();
    </script>
</body>
</html>

